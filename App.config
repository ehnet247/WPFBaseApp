<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- Logger configuration -->
    <add key="serilog:using:File" value="Serilog.Sinks.File" />
    <!--<add key="serilog:write-to:File.path" value="Logs\log_.txt" />-->
    <add key="serilog:write-to:File.path" value="log_" />
    <add key="serilog:write-to:File.rollingInterval" value="Day" />
    <add key="serilog:write-to:File.retainedFileCountLimit" value="50" />
    <add key="serilog:minimum-level" value="Warning" />
  </appSettings>
</configuration>